{
  "name": "ngrok Client",
  "version": "3",
  "slug": "ngrok",
  "description": "A ngrok client for Hass.io",
  "init": false,
  "webui": "http://[HOST]:[PORT:4040]",
  "url": "https://github.com/dy1io/hassio-addons",
  "stage": "deprecated",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "startup": "system",
  "boot": "auto",
  "ports": {
    "4040/tcp": "4040"
  },
  "ports_description": {
    "4040/tcp": "Web interface"
  },
  "options": {
    "log_level": "info",
    "auth_token": "",
    "region": "us",
    "tunnels": [
      {
        "name": "hass",
        "proto": "http",
        "addr": 8123,
        "inspect": false,
        "auth": ""
      }
    ]
  },
  "schema": {
    "log_level": "list(crit|warn|error|info|debug)",
    "auth_token": "str?",
    "region": "list(us|eu|ap|au|sa|jp|in)?",
    "tunnels": "str?"
  }
}
